<plugins xmlns="urn:org.androidaudioplugin.core" xmlns:pp="urn:org.androidaudioplugin.port">
  <plugin name="Flat Filter" category="Effect" author="atsushieno" manufacturer="atsushieno" unique-id="urn:org.androidaudioplugin/samples/aapbarebonepluginsample/FlatFilter" library="libaapbarebonepluginsample.so">
    <ports>
      <port direction="input" content="midi2" name="MIDI In" />
      <port direction="output" content="midi2" name="MIDI Out" />
      <port direction="input" content="audio" name="Left In" />
      <port direction="input" content="audio" name="Right In" />
      <port direction="output" content="audio" name="Left Out" />
      <port direction="output" content="audio" name="Right Out" />
      <port direction="input" content="control" name="Output Volume L" pp:default="0.5" pp:minimum="0" pp:maximum="1" />
      <port direction="input" content="control" name="Output Volume R" pp:default="0.5" pp:minimum="0" pp:maximum="1" />
      <port direction="input" content="control" name="Delay L" pp:default="0" pp:minimum="0" pp:maximum="2048" pp:type="integer" />
      <port direction="input" content="control" name="Delay R" pp:default="256" pp:minimum="0" pp:maximum="2048" pp:type="integer" />
      <port direction="input" content="control" name="Dummy Parameter 4" pp:default="50" pp:minimum="0" pp:maximum="100" pp:type="integer" pp:minimumSize="8192" />
      <port direction="input" content="control" name="Dummy Parameter 5" pp:default="50" pp:minimum="0" pp:maximum="100" pp:type="integer" />
      <port direction="input" content="control" name="Dummy Parameter 6" pp:default="50" pp:minimum="0" pp:maximum="100" pp:type="integer" />
      <port direction="input" content="control" name="Dummy Parameter 7" pp:default="50" pp:minimum="0" pp:maximum="100" pp:type="integer" />
    </ports>
    <!-- parameters extension -->
    <parameters xmlns="urn://androidaudioplugin.org/extensions/parameters">
      <parameter id="0" name="Output Volume L" default="0.5" minimum="0" maximum="1" />
      <parameter id="1" name="Output Volume R" default="0.5" minimum="0" maximum="1" />
      <parameter id="2" name="Delay L" default="0" minimum="0" maximum="2048" type="integer" />
      <parameter id="3" name="Delay R" default="256" minimum="0" maximum="2048" type="integer" />
      <parameter id="11" name="Dummy Parameter 4" default="50" minimum="0" maximum="100" type="integer" />
      <parameter id="12" name="Dummy Parameter 5" default="50" minimum="0" maximum="100" type="integer" />
      <parameter id="13" name="Dummy Parameter 6" default="50" minimum="0" maximum="100" type="integer" />
      <parameter id="14" name="Dummy Parameter 7" default="50" minimum="0" maximum="100" type="integer" />
    </parameters>
  </plugin>
</plugins>
